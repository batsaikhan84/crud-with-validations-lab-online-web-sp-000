<% form_for(@song) do |f|%>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :released %>
    <%= f.text_field :released %>
    <%= f.label :release_year %>
    <%= f.text_field :artist_name %>
    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.submit_form %>
<% end %>